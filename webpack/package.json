{
  "name": "webpack_react",
  "version": "1.1.0",
  "description": "webpack react scaffold",
  "author": "steven",
  "keywords": [
    "webpack",
    "react"
  ],
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "server": "set NODE_ENV=dev&& webpack-dev-server -d -w --colors --progress --hot --content-base ./dist",
    "dev": "set NODE_ENV=dev&& webpack -d --colors --progress --profile --display-error-details",
    "pro": "set NODE_ENV=pro&& webpack -p --colors --profile",
    "pro:abs": "set NODE_ENV=pro&& set NODE_ABS=abs&& webpack -p --colors --profile",
    "build": "npm run clean && npm run pro"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Eric--/webpack-react.git"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "prop-types": "^15.5.8",
    "raw-loader": "^0.5.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "config": {
    "port": "8080"
  }
}
